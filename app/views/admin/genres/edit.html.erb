<h3>ジャンル編集</h3>

<%= form_with model: @genre, url: admin_genre_path(@genre.id) do |f| %>
  <table>
    <tbody>
      <tr>
        <td>ジャンル名</td>
        <td>
          <%= f.text_field :name %>
        </td>
        <td>
          <%= f.submit '変更を保存' %>
        </td>
      </tr>
    </tbody>
  </table>


<% end %>