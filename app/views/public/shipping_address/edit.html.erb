<h3>配送先編集</h3>
<%= form_with model: @shipping_address, url: shipping_address_path do |f| %>
<div>
  <table>
    <tbody>
      <tr>
        <td>郵便番号(ハイフンなし)</td>
        <td><%= f.text_field :postal_code %></td>
      </tr>
      <tr>
        <td>住所</td>
        <td><%= f.text_area :address %></td>
      </tr>
      <tr>
        <td>宛名</td>
        <td><%= f.text_field :name %></td>
        <td><%= f.submit '変更内容を保存' %></td>
      </tr>
    </tbody>
  </table>
</div>
<% end %>